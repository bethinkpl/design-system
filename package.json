{
  "name": "@bethinkpl/design-system",
  "version": "11.1.1",
  "description": "Bethink universe design-system",
  "repository": "git@github.com:bethinkpl/design-system.git",
  "author": "nerdy@bethink.pl",
  "main": "./dist/design-system.umd.js",
  "typings": "./dist/index.d.ts",
  "license": "UNLICENSED",
  "engines": {
    "yarn": "^1.22.0",
    "node": "^14.15.0 || ^16.13.0"
  },
  "scripts": {
    "build": "vue-cli-service build --target lib --name design-system lib/js/index.ts",
    "dev": "start-storybook -p 6006",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:fix": "yarn lint:scripts:fix && yarn lint:styles:fix",
    "lint:scripts": "eslint --max-warnings 0 --cache 'lib/js/**/*.{js,vue,ts}'",
    "lint:scripts:fix": "eslint 'lib/js/**/*.{js,vue,ts}' --fix --cache",
    "lint:styles": "stylelint 'lib/**/*.{vue,scss}' --cache",
    "lint:styles:fix": "stylelint 'lib/**/*.{vue,scss}' --fix",
    "format:check": "prettier --check 'lib/**/*.{js,ts,vue,scss}' && prettier --check ./*.js",
    "format:fix": "prettier --write 'lib/**/*.{js,ts,vue,scss}' && prettier --write ./*.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o public/storybook",
    "test": "jest",
    "ts:check": "tsc --noEmit",
    "synchronizeColors": "npx ts-node tools/importers/SynchronizeColorsTokens.ts",
    "synchronizeTypography": "npx ts-node tools/importers/SynchronizeTypographyTokens.ts"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/pro-light-svg-icons": "6.1.1",
    "@fortawesome/pro-regular-svg-icons": "6.1.1",
    "@fortawesome/pro-solid-svg-icons": "6.1.1",
    "@fortawesome/pro-duotone-svg-icons": "6.1.1",
    "@fortawesome/vue-fontawesome": "2.0.6",
    "@storybook/addon-controls": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-storysource": "^6.5.13",
    "@storybook/addon-viewport": "^6.5.13",
    "@storybook/vue": "^6.5.13",
    "@types/node": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "@vue/cli-plugin-typescript": "^4.5.19",
    "@vue/cli-plugin-unit-jest": "^4.5.3",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.3.0",
    "axios": "^0.26.0",
    "babel-eslint": "10.1.0",
    "dotenv": "16.0.3",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-storybook": "0.6.6",
    "eslint-plugin-vue": "9.6.0",
    "jest": "26.1.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "postcss-prefix-selector": "^1.9.0",
    "prettier": "2.7.1",
    "sass": "^1.28.0",
    "sass-loader": "^7.1.0",
    "storybook-addon-designs": "^6.3.1",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "ts-jest": "^29.0.4",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4",
    "vue": "^2.6.11",
    "vue-jest": "^4.0.1",
    "vue-loader": "^15.9.6",
    "vue-popperjs": "catalystio/vue-popper#master",
    "vue-ripple-directive": "bethinkpl/vue-ripple-directive#2.1.1",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.12"
  }
}
