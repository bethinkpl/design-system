{
  "name": "design-system",
  "version": "1.0.0",
  "description": "Bethink univers design-system",
  "repository": "git@github.com:bethinkpl/design-system.git",
  "author": "nerdy@bethink.pl",
  "private": true,
  "scripts": {
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:scripts": "eslint --max-warnings 0 --cache 'src/js/**/*.{js,vue,ts}'",
    "lint:scripts:fix": "eslint 'src/js/**/*.{js,vue,ts}' --fix --cache",
    "lint:styles": "stylelint 'src/**/*.{vue,scss}' --cache",
    "lint:styles:fix": "stylelint 'src/**/*.{vue,scss}' --fix",
    "format:check": "prettier --check 'src/js/**/*.{js,ts,vue,scss}' && prettier --check ./*.js",
    "format:fix": "prettier --write 'src/js/**/*.{js,ts,vue,scss}' && prettier --write ./*.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@storybook/addon-controls": "^6.0.28",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/addon-storysource": "^6.0.28",
    "@storybook/addon-viewport": "^6.0.28",
    "@storybook/vue": "^6.0.28",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^4.5.3",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-vue": "7.7.0",
    "jest": "26.1.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.0.5",
    "resolve-url-loader": "2.3.1",
    "sass": "^1.28.0",
    "sass-loader": "^7.1.0",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "exports": {
    "js": "./src/js/",
    "styles": "./src/styles/"
  }
}
