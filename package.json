    "dotenv": "16.0.3",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-storybook": "0.6.6",
    "eslint-plugin-vue": "9.6.0",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
